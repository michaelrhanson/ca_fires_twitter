<script>
    var _vf = {},
        _vfs = {};

    _vfs.suppressRegistrationEmails = [
        'info@' + cphDomain,
        'judging@' + cphDomain,
        'XXXnewsroom@' + cphDomain
    ];

    _vfs.suppressRegistrationDomains = [
        'domain.tld'
    ];

    function vfSetDefaultVars() {
        _vf = {};
        _vf.isGuest = true;
        _vf.sessionId = false;
        _vf.hasValidName = false;
        _vf.isSuppressedEmail = false;
        _vf.suppressRegistration = false;
        _vf.avatarUrl = null;
        _vf.banMessage = null;
        _vf.focusDebounceTimer = null;
        _vf.avatarUrls = [];
    }

    function vfCookieSet(key, value) {
        document.cookie = key + '=' + value + '; path=/; domain=' + location.hostname + ';';
    }

    function vfCookieGet(key) {
        let escape = s => s.replace(/([.*+?\^${}()|\[\]\/\\])/g, '\\$1');
        let match = document.cookie.match(RegExp('(?:^|;\\s*)' + escape(key) + '=([^;]*)'));
        return match ? match[1] : null;
    }

    function vfCookieDelete(key) {
        document.cookie = key + '=; path=/; expires=Thu, 01 Jan 1970 00:00:01 GMT;';
        document.cookie = key + '=; path=/; domain=' + location.hostname + '; expires=Thu, 01 Jan 1970 00:00:01 GMT;';
        if(typeof cphCookieDomain === 'string')
            document.cookie = key + '=; path=/; domain=' + cphCookieDomain + '; expires=Thu, 01 Jan 1970 00:00:01 GMT;';
    }

    function vfGetEmail() {
        if(typeof _vf.Email !== 'string') {
            let vfEmail = cphGlobal.getCookieByName(cphPubAbbrev + '_email');

            vfEmail = jQuery.trim(vfEmail);
            vfEmail = vfEmail.toLowerCase();

            _vf.Email = vfEmail ? vfEmail : '';

            console.log('VF: Email set:', _vf.Email);
        }
        return _vf.Email;
    }

    function vfKillSession() {
        vfSetDefaultVars();
    }

    function vfIsSubscriber() {
        if(typeof _vf.IsSubscriber !== 'boolean')
            _vf.IsSubscriber = cphGlobal.getCookieByName('IsSubscriber') === 'True';

        return _vf.IsSubscriber;
    }

    function vfIsEmailSuppressed(email) {
        if(_vfs.suppressRegistrationEmails.indexOf(email) !== -1)
            return true;

        let vfRegDomain = email.split('@').pop();
        return _vfs.suppressRegistrationDomains.indexOf(vfRegDomain) !== -1;
    }

    function vfGetSessionId() {
        if(typeof _vf.sessionId !== 'string') {
            _vf.sessionId = '';
            if(vfIsSubscriber()) {
                let email = vfGetEmail();
                if(email) {
                    if(vfIsEmailSuppressed(email)) {
                        _vf.suppressRegistration = true;
                        _vf.isSuppressedEmail = true;
                        console.log('VF: ' + email + ' is suppressed');
                    } else {
                        _vf.sessionId = cphGlobal.md5(email);
                        console.log('VF: Session ID: ' + _vf.sessionId);
                    }
                }
            }
        }
        return _vf.sessionId;
    }

    function vfRenewInstance() {
        if(typeof window.vf === 'undefined') return;
        console.log('VF: renewing instance');
        vfCookieDelete('vf_session');
        window.vf.session.logout()
        .then(window.vf.context.teardown)
        .then(vfInit)
        .then(window.vf.context.setup);
    }

    function vfGetLists(callback) {
        if(_vfs.NameList && _vfs.BadList) {
            callback.call();
        } else {
            (function($) {
                let vfUrl = 'https://vfregister.pressdemocrat.com';
                $.when(
                    $.get(vfUrl + '/usernames.txt?time=' + (new Date()).getTime()),
                    $.get(vfUrl + '/badwords.txt?time=' + (new Date()).getTime())
                ).done(function(r1, r2) {
                    _vfs.NameList = r1.shift();
                    _vfs.BadList = r2.shift();
                    callback.call();
                });
            })(jQuery);
        }
    }

    function vfGetNameList() {
        return _vfs.NameList || '';
    }

    function vfGetBadList() {
        return _vfs.BadList || '';
    }

    function vfGetAvatarUrls() {
        if(!_vf.avatarUrls.length) {
            let vfSessionId = vfGetSessionId();
            console.log('VF: Fetching avatars for Session ID: ' + vfSessionId);

            $('#vfID').attr('value', vfSessionId);
            $('#vfEmail').attr('value', vfGetEmail());

            // if the generated icons will duplicate the default, offset to another group
            let md5last = vfSessionId.substring(vfSessionId.length - 1, vfSessionId.length),
                md5Offset = 0;

            if(parseInt(md5last) < 4) md5Offset = 4;

            let gravatarBase = 'https://www.gravatar.com/avatar/{{MD5}}?s=50&d=';

            let vfImage = gravatarBase.replace('{{MD5}}', vfSessionId) + 'identicon';

            $('#vfimage').attr('src', vfImage);
            vfSetAvatarUrl(vfImage);

            _vf.avatarUrls[0] = vfImage;

            let vfIconTypes = ['identicon', 'monsterid', 'wavatar', 'robohash'];
            let md5minus1 = vfSessionId.substring(0, vfSessionId.length - 1);

            let i, j, k;
            for(i = 0; i < 16; i++) {
                j = Math.floor(i / 4);
                k = (i % 4) + md5Offset;

                vfImage = gravatarBase.replace('{{MD5}}', md5minus1 + k) + vfIconTypes[j];
                $('#vfimage' + i).attr('src', vfImage);
                _vf.avatarUrls[i + 1] = vfImage;
            }
        }

        return _vf.avatarUrls;
    }

    function vfSetAvatarUrl(url) {
        _vf.avatarUrl = url;
    }

    function vfGetAvatarUrl() {
        return _vf.avatarUrl;
    }

    function vfIsGuest() {
        return _vf.isGuest;
    }

    function vfClickAvatar() {
        if(vfIsSubscriber() && !vfIsGuest())
            return;

        setTimeout(function() {
            window.vf.$publish('tray', 'close');
        });
    }

    function vfCommentFocus() {
        (function($) {
            if(!vfIsSubscriber()) {
                console.log('VF: calling login modal');

                cphLoader.loadCphLightbox(function() {
                    var buttons = [];
                    buttons.push(cphLightbox.button('Log In', {'data-mg2-action': 'login'}));
                    buttons.push(cphLightbox.button('Subscribe', null, function() {
                        location.href = 'https://subscribe.pressdemocrat.com';
                    }));

                    cphLightbox.hideAll();
                    $.cphDialog({
                        content  : 'You must be a subscriber to comment',
                        title    : 'Subscribers Only',
                        titleCss : 'background-color: #990000;',
                        showClose: true,
                        dialog   : {
                            buttons  : buttons,
                            buttonCss: 'color: #222222;'
                        }
                    });
                });

                return;
            }

            if(_vf.banMessage) {
                console.log('VF: Opening ban message modal');

                cphLoader.loadCphLightbox(function() {
                    cphLightbox.hideAll();
                    $.cphDialog({
                        content : _vf.banMessage,
                        title   : 'Please Note',
                        titleCss: 'background-color: #990000;',
                        width   : 500
                    });
                });

                return;
            }

            if(!vfIsGuest()) return;

            if(_vf.suppressRegistration) {
                console.log('VF: Registration modal suppressed');

                if(_vf.isSuppressedEmail) {
                    cphLoader.loadCphLightbox(function() {
                        cphLightbox.hideAll();
                        $.cphDialog({
                            content : 'Commenting is turned off for this account.',
                            title   : 'Please Note',
                            titleCss: 'background-color: #990000;',
                            width   : 350
                        });
                    });
                }

                return;
            }

            cphLoader.loadCphLightbox(function() {
                console.log('VF: Opening registration modal');

                vfGetAvatarUrls();
                vfGetLists(function() {
                    cphLightbox.hideAll();
                    $.cphLightbox({
                        content            : '#vfregister',
                        width              : 400,
                        closeOnClickOutside: false,
                        closeOnEscape      : false
                    });
                });
            });
        })(jQuery);
    }

    function vfNameFocus() {
        jQuery('#nameErr').empty();
    }

    function vfNameBlur(debounceTimeout) {
        (function($) {
            // function is debounced because it is tied to a keyup event,
            // so it will only fire after 'debounceTimeout' ms after the last keystroke
            clearTimeout(_vf.NameBlurDebounce);
            _vf.NameBlurDebounce = setTimeout(function() {
                let name = $('#vfName').val();

                name = name.replace(/^\s+|\s+$/, '');
                name = name.replace(/\s+/, ' ');
                name = name.replace(/-+/, '-');

                _vf.hasValidName = false;

                $('#vfName').val(name);
                $('#nameErr').empty();
                $('#vfSubmit').prop('disabled', true);

                if(!name.length) return;

                if(!(/^[a-zA-Z0-9\s\-]+$/.test(name))) {
                    $('#nameErr').text('Names can only contain numbers, letters, spaces and dashes');
                    return;
                }

                if(typeof _vf.NamesLower === 'undefined')
                    _vf.NamesLower = vfGetNameList().toLowerCase().split(',');

                if(typeof _vf.BadWordsLower === 'undefined')
                    _vf.BadWordsLower = vfGetBadList().toLowerCase().split(',');

                let scrubbedName = name.toLowerCase();
                scrubbedName = scrubbedName.replace(/\s/, '');
                scrubbedName = scrubbedName.replace(/-/, '');

                // check if name is already used or not allowed, regardless of case
                if(_vf.NamesLower.indexOf(name) !== -1) {
                    $('#nameErr').text('Sorry, that name has been taken');
                    return;
                }

                // check if banned words are used anywhere in the name
                let vfHasBadWords = false;
                $.each(_vf.BadWordsLower, function(i, word) {
                    if(scrubbedName.indexOf(word) !== -1) {
                        $('#nameErr').text('That name is not allowed');
                        vfHasBadWords = true;
                        return false;
                    }
                });

                if(vfHasBadWords) return;

                _vf.hasValidName = true;
                $('#vfSubmit').prop('disabled', false);
            }, debounceTimeout);
        })(jQuery);
    }

    function vfSubmitHandler() {
        event.preventDefault();

        (function($) {
            if(!_vf.RegFormSubmitting) {
                _vf.RegFormSubmitting = true;

                $('#vfSubmit').prop('disabled', true);

                if(!_vf.hasValidName) {
                    $('#nameErr').html('A valid display name is required');
                    return;
                }

                let vfName = $('#vfName').val(),
                    vfEmail = vfGetEmail(),
                    vfSession = vfGetSessionId(),
                    vfAvatarUrl = vfGetAvatarUrl(),
                    vfLoader = $('#vf-loader');

                if(!(vfName && vfEmail && vfSession && vfAvatarUrl)) {
                    $('#nameErr').html('Registration failed');
                    return;
                }

                console.log('VF: Registering user');

                vfLoader.addClass('show');

                let getUrl = 'https://vfregister.pressdemocrat.com/register_me.php?session=' + encodeURIComponent(vfSession) + '&email=' + encodeURIComponent(vfEmail) + '&name=' + encodeURIComponent(vfName) + '&avatar=' + encodeURIComponent(vfAvatarUrl);

                $.get(getUrl).done(function(data) {
                    console.log('VF: Submission status: ' + data);

                    if(data === 'ok') {
                        cphLightbox.hideAll();
                        console.log('VF: Registration complete');
                        vfRenewInstance();
                        return;
                    }

                    console.log('VF: Registration failed');
                }).fail(function() {
                    console.log('VF: Registration failed');
                }).always(function() {
                    _vf.RegFormSubmitting = false;
                    vfLoader.removeClass('show');
                });
            }
        })(jQuery);
    }

    function vfPickAvatar(val) {
        (function($) {
            let e = event;
            if(e.type === 'keydown') {
                if(!(e.keyCode === 13 || e.keyCode === 32 || e.key === 'Enter' || e.key === ' ')) {
                    return; // only continue if keydown is triggered by Spacebar or Enter
                }
            }

            $('.pickBox').css('border-color', 'white');
            $('#pickBox' + val).css('border-color', 'red');

            let offset = 0;
            if(val !== 'D')
                offset = val + 1;

            let avatarUrls = vfGetAvatarUrls(),
                avatarUrl = avatarUrls[offset] || '';

            if(avatarUrl) vfSetAvatarUrl(avatarUrl);
        })(jQuery);
    }

    function vfInit() {
        console.log('VF: -------- Init called --------');

        vfKillSession();

        let sessionId = vfGetSessionId();

        //window.vfQ = window.vfQ || [];
        window.vfQ = [];

        window.vfQ.push(function() {
            if(sessionId) {
                console.log('VF: Logging in');
                vfCookieSet('vf_session', sessionId);
                window.vf.session.login.cookie(sessionId);
            } else {
                console.log('VF: Making sure user is logged out');
                vfCookieDelete('vf_session');
                vf.session.logout();
            }
        });

        window.vfQ.push(function() {
            window.vf.$subscribe('login', 'success', function(userContext) {
                console.log('VF: User name:', userContext.name);
                // console.log('VF: User:', userContext);
                // userContext.ban_level: "posting"
                // userContext.ban_level: "ghost"
                // userContext.ban_level: "none"
                _vf.isGuest = userContext.user_privilege === 'guest';
                _vf.suppressRegistration = true;
                $('#ban-reason').hide();
            });
        });

        window.vfQ.push(function() {
            window.vf.$subscribe('login', 'failure', function(errorContext) {
                //console.log('VF: login failed:', errorContext);
                if(errorContext.http_status === 401) {
                    console.log('VF: login failed: unauthorized');
                }
                if(errorContext.error_key === 'login_banned') {
                    if(errorContext.isPermanent) {
                        console.log('VF: login failed: permanent ban');
                        _vf.banMessage = '<p>Your access to the comments has been permanently suspended.</p>';
                    } else {
                        console.log('VF: login failed: temporary ban');
                        _vf.banMessage = '<p>Your access to the comments has been temporarily suspended for the following reason(s):</p><p><em style="color: #666666;">' + errorContext.reason + '</em></p><p style="font-size:.9em;color:#333333;">The ban will expire at ' + new Date(errorContext.endDate) + '.</p>';
                    }
                    $('#ban-reason').html('<h4>Please note</h4>' + _vf.banMessage).show();
                }
            });
        });

        $(document).trigger('vf:loaded');
    }

    function vfDebounce(func) {
        return function() {
            var context = this, args = arguments;
            clearTimeout(_vf.focusDebounceTimer);
            _vf.focusDebounceTimer = setTimeout(function() {
                _vf.focusDebounceTimer = null;
                func.apply(context, args);
            }, 150);
        };
    }

    function vfSetListeners() {
        (function($) {
            vfSetDefaultVars();

            $(document).off('.vf');

            $(document).on('focus.vf', '.vf-content-editor-input', vfDebounce(vfCommentFocus));

            $(document).on('click.vf', 'button.vf-post-form__avatar, button.vf3-reply-with-new__avatar', vfDebounce(vfClickAvatar));

            $(document).on('mg2LoggedOut.vf', function() {
                console.log('VF: mg2LoggedOut called');
                $('#ban-reason').empty().hide();
                vfKillSession();
                vfRenewInstance();
            });

