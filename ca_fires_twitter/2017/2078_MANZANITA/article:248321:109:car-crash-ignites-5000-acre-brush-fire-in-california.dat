                ga('send', 'event', 'Image', 'Full Screen');
                articleSlider.pause();
            });

            // Setup Media Player
            jQuery('audio').mediaelementplayer({
                audioWidth: '100%',
                success: function (media) {
                    media.addEventListener('play', function () {
                        ga('send', 'event', 'Audio', 'Play');
                        articleSlider && articleSlider.pause && articleSlider.pause();
                    }, true);
                    media.addEventListener('pause', function () {
                        ga('send', 'event', 'Audio', 'Pause');
                        articleSlider && articleSlider.pause && articleSlider.pause();
                    }, true);
                    media.addEventListener('ended', function () {
                        ga('send', 'event', 'Audio', 'Played To End');
