        function lazyLoad_check(obj) {
            //check if jquery obj was passed
            //if so we'll check only inside that element
            if (obj) {
                //convert jquery obj to simple element
                obj = obj[0];
            } else {
                //just search the whole page is nothing passed
                obj = document;
            }
            if ('loading' in HTMLImageElement.prototype) {
                //check for native browser lazing load functionality
                const images = obj.querySelectorAll('img[loading="lazy"], iframe[loading="lazy"]');
                images.forEach(img => {
                    img.src = img.dataset.src;
                });
            } else {
                // otherwise Dynamically import the LazySizes library
                const script = obj.createElement('script');
                script.src =
                    'https://cdnjs.cloudflare.com/ajax/libs/lazysizes/5.1.2/lazysizes.min.js';
                document.body.appendChild(script);
            }
        }
        lazyLoad_check();
