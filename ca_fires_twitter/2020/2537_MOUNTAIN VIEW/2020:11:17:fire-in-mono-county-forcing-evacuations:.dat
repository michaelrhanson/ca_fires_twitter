 with a downed power line."},{"_id":"MBLIAPLEYJC67J3FIJMZ33345I","type":"text","additional_properties":{"_id":"X5UHFZZKR5AQDILHUEXBT3APSY","comments":[],"inline_comments":[]},"content":"<i>Copyright 2020 KOLO. All rights reserved.</i>"}],"created_date":"2020-11-17T21:34:12.855Z","revision":{"revision_id":"XTQJJAP2GRCTXKPHSE5H5TH2PQ","parent_id":"VTPSPUXV3JFEXGK2FRLPFVLKJ4","editions":["default"],"branch":"default","user_id":"steve.timko@kolotv.com","published":true},"last_updated_date":"2020-11-22T19:39:56.023Z","canonical_url":"/2020/11/17/fire-in-mono-county-forcing-evacuations/","headlines":{"basic":"Mountain View Fire containment grows to 65%","mobile":"","native":"","print":"","tablet":"","web":"","meta_title":""},"owner":{"sponsored":false,"id":"gray"},"copyright":"Copyright 2020 KOLO. All rights reserved.","address":{},"workflow":{"status_code":1},"subheadlines":{"basic":""},"description":{"basic":"The containment line is mostly complete for areas not under snow."},"language":"","label":{},"source":{"name":"gray","system":"composer","source_type":"staff"},"taxonomy":{"tags":[],"sites":[],"sections":[{"_id":"/news/fire","_website":"kolo","type":"section","version":"0.6.0","name":"Fire","description":"Fire","path":"/news/fire","parent_id":"/news","parent":{"default":"/news"},"additional_properties":{"original":{"_id":"/news/fire","site":{"site_keywords":null,"site_description":"Fire","site_url":null,"site_title":"Fire","site_about":null,"site_tagline":"Fire","pagebuilder_path_for_native_apps":null,"section_promo_image":null},"social":{"twitter":null,"facebook":null,"rss":null,"instagram":null,"og_image_url":null},"site_topper":{"site_logo_image":null},"navigation":{"nav_title":"Fire"},"_admin":{"dfp_id":"/news/fire/","wallpaper_ad":"false","alias_ids":["/news/fire"]},"name":"Fire","_website":"kolo","parent":{"default":"/news","publishing-sections":"/"},"ancestors":{"default":["/news"],"publishing-sections":["/"]},"inactive":false,"node_type":"section","order":{