function fireLevelClass(l){
	var ret;
	switch (l){
		case '1':
			ret='dllow';	
			break;
		case '2':
			ret='dlmoderate';
			break;
		case '3':
			ret='dlhigh';
			break;
		case '4':
			ret='dlveryhigh';
			break;
		case '5':
			ret='dlextreme';
			break;
		default :
			ret='dllow';
			break;
	}
	return ret;
	
};

function ifplRoman(ifpl){
	var ret;
	switch(ifpl){
		case '1':
			ret='I';	
			break;
		case '2':
			ret='II';
			break;
		case '3':
			ret='III';
