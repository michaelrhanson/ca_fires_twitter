                ylen = stories.length,
                els,
                split,
                toRemove,
                newel,
                x,
                xlen;
            for(y = 0; y < ylen; y += 1) {
                els = (!!stories && !!stories[0]) ? jQuery(stories[y]).find(".homestory") : [];
                split = Math.floor(els.length/2);
                toRemove = els.splice(-1 * split, split);
                newel = $("<div class='stream-container-right'></div>");
                if (stories.length > 0 && els.length > 3) {
                    xlen = toRemove.length;
                    if (toRemove.length > 0) {
                        $(toRemove[0]).parent().parent().append(newel);
                        for (x = 0; x < xlen; x += 1) {
                            $(toRemove[x]).detach().appendTo(newel);
                        }
                    }
                }
            }
