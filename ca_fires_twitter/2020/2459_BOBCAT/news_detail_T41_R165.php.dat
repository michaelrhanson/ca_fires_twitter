	// ready
	$(function(){

		function getCleanString(str){
			if(str.length){
				return str.replace(/&lt;/g,"<").replace(/&gt;/g,">").replace(/&amp;/g,"&").replace(/&quot;/g,"\"").replace(/&#39;/g,"\'");
			} else {
				return "";
			}
		}

		var searchStr = "";
		var throttle = null;
		var timer;
		var $input = $('#search-input');
		var termsArr = [];
		//on keyup, start the countdown
		$input.on('keyup', function () {
		  clearTimeout(timer);
		  timer = setTimeout(liveSearch, 600);
		  searchStr = $(this).val().toLowerCase();
		});
		$input.on('keydown', function () {
			termsArr = [];
		  clearTimeout(timer);
		});

		function liveSearch () {
			var libCheck = $('body').hasClass('library-homepage') && $('#slider-header').attr('data-search-type') === 'website';
			if($('body').hasClass('library-homepage')){
				if($('#slider-header').attr('data-search-type') === 'website') libCheck = true;
				else libCheck = false;
			} else {
				libCheck = true;
			}

			if(searchStr.length >= 3 && libCheck){
				$('#search-results').fadeIn();
				$('#slider').addClass('results-open');
				termsArr = searchStr.split(' ');
				//$('#curated').fadeOut();
				$('#curated').html('');
				$.each(searchJson,function(i,el){
					var showRecord = true;
					$.each(termsArr,function(k,str){
						if(el.terms.indexOf(str)  < 0) showRecord = false
					});
					if(showRecord){
						var tags = "";
						if(el.tags !== ""){
							tags = el.tags.split(",")
								.map(function(val){
									return '<span>'+val+'</span>';
							}).join('');
						}

						$('#curated').append('<div class="curated-item clearfix">'
						+ '<div class="pull-right">'
						+ (el.url === '#' ? '<h4>'+el.subheader+'</h4>' : '<a class="search-dept-url" href="'+el.deptUrl+'" target="'+el.deptTarget+'"><h4>'+el.subheader+'</h4></a>')
						+ (el.phone === '' ? '' : '<a class="search-icon" href="tel:'+el.phone+'"><i class="fa fa-phone"></i></a>')
						+ (el.email === '' ? '' : '<a class="search-icon" href="mailto:'+el.email+'"><i class="fa fa-envelope"></i></a>')
						+ (el.address === '' ? '' : '<a class="search-icon" href="maps.google.com/?q='+el.address+'"><i class="fa fa-map-marker"></i></a>')
						+ '</div>'
						+ '<h3 class="pull-left"><a href="'+el.url+'" target="'+el.target+'" class="clearfix">'+el.header+'</a><small style="display:block">'+el.description+'</small></h3>'
						+ '<div class="curated-tags">'+tags+'</div>'
						+ '</div>');
					}
				});
				if(!$('.curated-item').length){
					$('#curated').append('<div class="curated-item"><h3>No keywords found. Use a different keyword or click on the All of Arcadia tabâ¦</h3></div>');
				}
			} else {
				$('#search-results').fadeOut();
				$('#slider').removeClass('results-open');
			}
		}


	});
</script>





