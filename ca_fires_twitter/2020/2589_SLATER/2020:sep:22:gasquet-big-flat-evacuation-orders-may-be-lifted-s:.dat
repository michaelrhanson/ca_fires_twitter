        conn.onmessage = function(evt) {
            var obj = JSON.parse(evt.data);
            var notification = $('#notifications');
            if (obj.model === 'blog.post') {
              $.event.trigger({
                 'type': 'loco.notification',
                 'title': 'New post: ' + obj.fields.title,
                 'url': obj.url,
                 'model': 'blog.post'
              });
