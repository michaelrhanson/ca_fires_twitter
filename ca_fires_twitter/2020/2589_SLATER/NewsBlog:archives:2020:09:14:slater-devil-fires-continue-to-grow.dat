          if (!myStart) var myStart = "1";
          var mySort = jQuery("#sortSelect").val() || "desc";
          var params = {
            sort: mySort,
            ajaxComponent: componentId,
            startIndex: myStart,
            showAll: showAllComments
          };
          jQuery.ajax({
              url: url,
              data: (params),
