            if ('loading' in HTMLImageElement.prototype) {
                //check for native browser lazing load functionality
                const images = obj.querySelectorAll('img[loading="lazy"], iframe[loading="lazy"]');
                images.forEach(img => {
                    img.src = img.dataset.src;
