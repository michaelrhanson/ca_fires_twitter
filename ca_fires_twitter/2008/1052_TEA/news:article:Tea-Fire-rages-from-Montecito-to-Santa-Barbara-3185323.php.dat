    "@type": "NewsArticle",
    "mainEntityOfPage": "https:\/\/www.sfgate.com\/news\/article\/Tea-Fire-rages-from-Montecito-to-Santa-Barbara-3185323.php",
    "url": "https:\/\/www.sfgate.com\/news\/article\/Tea-Fire-rages-from-Montecito-to-Santa-Barbara-3185323.php",
    "headline": "Tea Fire rages from Montecito to Santa Barbara",
    "image": {
        "@type": "ImageObject",
        "url": "https:\/\/s.hdnux.com\/photos\/10\/67\/06\/2322868\/6\/rawImage.jpg",
