<meta itemprop="name" content="100+ Women Strong in Lake County" id="dp-module-upcoming-default-210-events-6818-6818-name">
<meta itemprop="startDate" content="2023-02-09T02:00:00+00:00" id="dp-module-upcoming-default-210-events-6818-6818-start-date">
