<script>
var MTM_Paywall_Promotions = (function(cookies, undefined){
    'use strict';

    init();

    function init(){
        var all_promotions = [];
        var user_cookie = findMSPANCookie();
        var expire = new Date();

        if( ! user_cookie ){
            return;
        }
    
        if( 'undefined' === typeof user_cookie['type'] ){
            return;
        }
    
        if('promotion' === user_cookie['type']){

            expire.setFullYear(expire.getFullYear() + 1);
            
            all_promotions.push(user_cookie['type']);
            document.cookie = 'MSPAN_Paywall_Promotions=' + JSON.stringify(all_promotions) + '; path=/; expires=' + expire.toGMTString();
        }
        return;
    }    

    function findMSPANCookie(){
        for(var i = 0; i < cookies.length; i++){
            if(cookies[i].trim().match(/MSPAN_Paywall_user/i)){
                try{
                    return JSON.parse(decodeURIComponent(cookies[i].split('=')[1]));
                }catch(ex){
                    return false;
                }
            }
        }
        return false;
    }

    return {
        get: function(){
            return findMSPANCookie();
        }
    };

}(document.cookie.split(';')));
</script></div></div><div id="custom_html-58" class="widget_text widget widget_custom_html"><div class="textwidget custom-html-widget"><style>
