<script type="text/javascript">
  (function(){
    let nodeId = "291795";
    let groupsDiv = document.getElementById("groups__div");
    let groupHeader = document.getElementById("groups__header");
    let groupContent = document.getElementById("groups__content");

    function stripWord(str) {
      return str.toLowerCase().split(" ").join("-");
    }

    fetch("/api/util/group-header")
      .then(res => res.json())
      .then((data) => {

        // Check for incidents
        if (data.length < 1) {
          return;
        }

        // let incidents = data.filter(d => d.data.nid[0]['value'] == nodeId);
        let groups = data.block.filter(d => d.incident_id == nodeId);
        console.log(groups);

        // Change header
        let groupType = groups[0].group_type_name;
        groupHeader.innerHTML = `${groupType} Incidents`;

        // Add content
        groups.forEach(group => {
          groupContent.innerHTML += `<a target="_blank" href="/incident-group/${group.group_id}" style="display: block">${group.group_name}</a>`
        });

        // Show content
        groupsDiv.style = "display: block";
      });
    }
  ());
</script>

