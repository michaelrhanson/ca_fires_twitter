<script>
    var baseLanguage = 'en';
    function setPreferredLanguage(language) {
        var search = window.location.search.replace(/([?&])lang=[^&#]+(&|$)/g, "$1").replace(/\?$/, "");

        if (language === baseLanguage) {
            document.cookie = 'CAL_FIRE_Translate_To=; expires=Thu, 01 Jan 1970 00:00:00 UTC; path=/';
        } else {
            var d = new Date();
            d.setTime(d.getTime() + (365 * 24 * 60 * 60 * 1000));

            document.cookie = 'CAL_FIRE_Translate_To=' + language + '; expires=' + d.toUTCString() + '; path=/';
            search += (search.length > 0 ? '&' : '?') + 'lang=' + language;
        }

        if (search !== window.location.search) {
            if (search.length > 0) window.location.search = search;
            else window.location = window.location.origin + window.location.pathname + window.location.hash;
        } else window.location.reload();
    }

    document.addEventListener("click", function (e) {
        if (e.target.matches(".translation-header-toggle")) {
            toggleTranslationHeader();
        }
    });

</script>

