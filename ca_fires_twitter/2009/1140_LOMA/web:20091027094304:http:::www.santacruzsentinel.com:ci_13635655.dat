<script>

function pmc_handle_multipolls() {
	var tables = document.getElementsByTagName("table");

	for (var i = 0 ; i < tables.length; i++) {
		if (tables[i].className != "pollsBox") continue;
		var inputs = tables[i].getElementsByTagName("input");
		for (var j = 0; j < inputs.length; j++) {
			if (inputs[j].type != "button" && inputs[j].type != "submit") {
				continue;
			} else if (inputs[j].type == "button") {
				var b = inputs[j];
				var f = b.onclick.toString();
				var fn = f.substring(f.indexOf("{") + 1,f.lastIndexOf("()"));
				fn = fn.replace(/[\n\s]+/,"");
				var pre = fn.replace("vote","answer") + "_"
				pre = pre.replace(/[\n\s]+/,"");
				eval("var fun = " + fn + ";");
				var funtext = fun.toString();
				
				funtext = funtext.replace("{","{\nvar prefix = \"" + pre + "\";");
				funtext = funtext.replace("function " + fn,"");
				eval ("b.onclick = function " + funtext);				
			}
		}//** end for loop
	}
}



