		var o = ivl.href;
		var n = "javascript: void window.open('" + o + "','ngps_gallery_win','width=600,height=500,resizable=1');";
		ivl.target = "_top";
		ivl.href = n;

		alterLink = PMCalterLink;

	} else return true;
}

function PMCalterLink() { 
	if (viewer_allowEnlargement) { 
		var fullUrl = viewer_galleryUrl; 
		if (viewer_galleryUrl.indexOf("?") == -1) { fullUrl += "?"; } else { fullUrl += "&"; } 
		fullUrl += "articleId="; 
		fullUrl += viewer_articleId; 
		fullUrl += "&siteId="; 
		fullUrl += viewer_siteId; 
		fullUrl += "&"; 
		if ("true" == viewer_isPreviewing) { fullUrl += "isPreviewing=true&"; } 
		if ("true" == viewer_isEmbedded) { fullUrl += "isEmbedded=true&"; } 
		fullUrl += "startImage="; 
		fullUrl += viewer_currentlySelected; 
		$("gallery_link").href = "javascript: void window.open('" + fullUrl + "','ngps_gallery_win','width=600,height=500,resizable=1');"; 
	} 
}

