        case 'Breaking News':leftColor='cnnBlack';rightColor='cnnYellow';break;
        case 'Live Developing Story':leftColor='cnnYellow';rightColor='cnnBlack';flashURL=cdn_prefix+'/.element/swf/2.0/breaking_news/bn_dev.swf';break;
